{
  "extends": ["jhipster-react"],
  "parserOptions": {
    "project": "./tsconfig.e2e.json"
  },
  "rules": {
    "@typescript-eslint/no-this-alias": [
      "error",
      {
        "allowDestructuring": true, // Allow `const { props, state } = this`; false by default
        "allowedNames": ["self"] // Allow `const self = this`; `[]` by default
      }
    ]
  }
}
